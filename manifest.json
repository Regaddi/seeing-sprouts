{
  "name": "Seeing Sprouts",
  "version": "0.1",
  "description": "Browser extension that replaces every image with brussel sprouts.",
  "manifest_version": 2,
  "web_accessible_resources": ["*.jpg"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["sproutify.js"]
    }
  ],
  "icons": {
    "48": "images/icon-sprout-48.png",
    "96": "images/icon-sprout-96.png"
  }
}
